// Was going to make header as a function because it would have be faster to
// check than checking if same string.
//
//   String comparison: Check type is same, check if all characters are same.
//   Function comparison: Check type is same, check if pointing at same function.
//
// Except when going over the use<> boundary, something happens.  Function
// returned is different every time a function is called, even if the function
// returned is a global variable.  Not sure why.  Works locally, but not between
// files.  Value is copied every time a function is called.
/** Used to indicate if a list object is identified as an `_SLR_CACHE` object */
_SLR_CACHE_HEADER = "_SLR_CACHE";

/**
 * _slr_cache(slr, birl, end_i)[_SLR_LEN] (number)
 *   Value representing the length of `slr`.
 */
_SLR_LEN  = 1;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_TE] (type_enum)
 *   Value representing the type of `slr`.
 */
_SLR_TE   = 2;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_ELD] (function(i): any)
 *   Function dereferences the `slr` DIRECTLY.  E.g. index directly to `slr`
 *   without going through the BIRLEI.
 */
_SLR_ELD  = 3;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_BLEN] (number)
 *   Length of BIRLEI.
 */
_SLR_BLEN = 4;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_ELI] (function(i): any)
 *   Function dereferences the `slr` INDIRECTLY by getting index through the
 *   BIRLEI, and using that index to index the `slr`.
 */
_SLR_ELI  = 5;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_IDX] (function(i): number)
 *   Function that dereferences the BIRLEI value.
 */
_SLR_IDX  = 6;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_STR] (function() : string)
 *   Function converting normalised BIRLEI to string.
 */
_SLR_STR  = 7;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_BIRL] (number | list | range)
 *   Normalised BIRL component of BIRLEI.
 */
_SLR_BIRL = 8;

/**
 * _slr_cache(slr, birl, end_i)[_SLR_END_I] (number | undef)
 *   Normalised END_I component of BIRLEI.
 */
_SLR_END_I = 9;

